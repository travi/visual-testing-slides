{
  "name": "visual-testing-slides",
  "description": "Spectacle slide deck for talk about visual testing",
  "license": "MIT",
  "private": true,
  "repository": "travi/visual-testing-slides",
  "bugs": "https://github.com/travi/visual-testing-slides/issues",
  "homepage": "https://github.com/travi/visual-testing-slides#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "predeploy": "run-s build",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./lib/ --prod",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "clean": "rimraf ./lib",
    "start": "webpack-dev-server",
    "prebuild": "run-s clean",
    "build": "webpack --env production",
    "build:dev": "webpack --env development"
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-hot-loader": "4.12.16",
    "redbox-react": "1.6.0",
    "spectacle": "6.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/register": "7.7.0",
    "@travi/babel-preset": "3.0.6",
    "@travi/eslint-config": "1.0.25",
    "@travi/eslint-config-react": "1.0.19",
    "babel-loader": "8.0.6",
    "ban-sensitive-files": "1.9.2",
    "commitlint-config-travi": "1.3.4",
    "cz-conventional-changelog": "3.0.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "3.0.9",
    "lockfile-lint": "2.2.0",
    "mustache-loader": "1.4.3",
    "netlify-cli": "2.20.2",
    "npm-run-all": "4.1.5",
    "raw-loader": "3.1.0",
    "remark-cli": "7.0.1",
    "remark-preset-lint-travi": "1.3.0",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "travis-lint": "1.0.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.12",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "3.9.0"
  }
}
